{
  "expo": {
    "name": "Aedes em Foco Agente de Saúde",
    "slug": "aedes-agent-saude-react-native",
    "privacy": "unlisted",
    "sdkVersion": "34.0.0",
    "platforms": [
      "android"
    ],
    "version": "0.6.2",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/SplashScreen.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "packagerOpts": {
      "config": "./rn-cli.config.js",
      "projectRoots": "",
      "sourceExts": [
        "js",
        "jsx",
        "styl"
      ]
    },
    "android": {
      "package": "br.ufc.lme.aedesemfoco.saude",
      "versionCode": 2,
      "permissions": [
        "ACCESS_FINE_LOCATION"
      ]
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "extra": {
      "baseurl": "https://aedes-acs-production.herokuapp.com/"
    },
    "description": "Aplicativo para auxiliar os agentes de saúde de cedro no monitoramento da saúde da população"
  }
}
